{
  "name": "soknad-kontantstotte",
  "version": "2.0.0",
  "description": "Søknad om kontantstøtte",
  "type": "module",
  "scripts": {
    "format-all": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "start:dev": "tsc -p tsconfig.backend.json && NODE_ENV=development ENV=localhost node --es-module-specifier-resolution=node build/backend/server.js",
    "start": "NODE_ENV=production node --es-module-specifier-resolution=node build/backend/server.js",
    "build": "TS_NODE_PROJECT=src/webpack/tsconfig.json NODE_ENV=production webpack --config src/webpack/webpack.production.config.ts && tsc -p tsconfig.backend.json",
    "build:analysis": "TS_NODE_PROJECT=src/webpack/tsconfig.json NODE_ENV=production webpack --config src/webpack/webpack.analysis.config.ts",
    "typecheck-frontend": "tsc --noEmit -p src/frontend/tsconfig.json",
    "test": "jest",
    "test:ci": "./run-ci-tests.sh test",
    "test:ci-update": "./run-ci-tests.sh update",
    "start:ci-frontend": "NODE_ENV=production node --es-module-specifier-resolution=node build/backend/ci-frontend-server.js",
    "start:ci-api-mock": "node --es-module-specifier-resolution=node build/backend/ci-api-mock.js",
    "clean": "rm -rf dist build"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/src/mockStyle.js",
      "nav-(.*)-style": "<rootDir>/src/mockStyle.js",
      "nav-frontend-core": "<rootDir>/src/mockStyle.js",
      "styled-components/macro": "styled-components"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@navikt)/)",
      "\\.pnp\\.[^\\/]+$"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "resetMocks": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run typecheck-frontend"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "resolutions": {
    "styled-components": "^5",
    "browserslist": "^4.16.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
